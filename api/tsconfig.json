{
    "compilerOptions": {
        "incremental": true,
        "target": "es5",
        "module": "commonjs",
        "lib": [
          "esnext.asynciterable"
        ],
        "sourceMap": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    },
    "include": [
        "src"
    ]
}
