{
    "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "module": "esnext",
    "lib": [
        "dom",
        "dom.iterable",
        "esnext"
    ],
    "jsx": "react",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
    },
    "include": [
        "src"
    ]
}
